<odoo>
    <record model="ir.module.category" id="cmdb_category">
        <field name="name">Inventario Técnico</field>
        <field name="description">Niveles de acceso al modulo Inventario Técnico</field>
        <!--<field name="sequence">20</field>-->
    </record>

    <record id="root_group" model="res.groups">
        <field name="name">Administrador / Root</field>
        <field name="category_id" ref="cmdb_category"/>
    </record>

    <record id="admin_technician_group" model="res.groups">
        <field name="name">Administrador / Técnico Radar</field>
        <field name="category_id" ref="cmdb_category"/>
    </record>

    <record id="admin_atc_group" model="res.groups">
        <field name="name">Administrador / Controlador</field>
        <field name="category_id" ref="cmdb_category"/>
    </record>

    <record id="technician_group" model="res.groups">
        <field name="name">Técnico Radar</field>
        <field name="category_id" ref="cmdb_category"/>
    </record>

    <record id="atc_group" model="res.groups">
        <field name="name">Controlador</field>
        <field name="category_id" ref="cmdb_category"/>
    </record>


</odoo>
